<h2>New User Sign Up, Create Username and Password</h2>
<br>


<%= form_for @user do |f|%>

<%= f.label :username %><br>
<%= f.text_field :username %><br>

<%= f.label :password %><br>
<%= f.password_field :password %><br>

<%=f.label :password_confirmation, "Confirmation" %><br>
<%=f.password_field :password_confirmation %><br>

<%= f.submit "Join"%>

<%end%>
